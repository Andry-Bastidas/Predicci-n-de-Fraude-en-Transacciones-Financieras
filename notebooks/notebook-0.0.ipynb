   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Transformaciones ciclicas para datos temporales"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "data[\"hour_sin\"] = np.sin( 2 * np.pi * (data[\"hour\"] / 24))\n",
    "data[\"hour_cos\"] = np.cos( 2 * np.pi * (data[\"hour\"] / 24))\n",
    "\n",
    "data[\"week_sin\"] = np.sin( 2 * np.pi * (data[\"week\"] / 7))\n",
    "data[\"week_cos\"] = np.cos( 2 * np.pi * (data[\"week\"] / 7))\n",
    "\n",
    "data['month_sin'] = np.sin(2 * np.pi * (data['month'] / 12.0))\n",
    "data['month_cos'] = np.cos(2 * np.pi * (data['month'] / 12.0))"
   ]
  },
