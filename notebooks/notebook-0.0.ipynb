  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [],
   "source": [
    "data['dob'] = pd.to_datetime(data['dob'] , format='%Y-%m-%d')\n",
    "data['year2'] = data['dob'].dt.year\n",
    "data['age'] = data[['year2' , 'year']].apply(\n",
    "    lambda x : x['year'] - x['year2'] , axis = 1\n",
    ")"
